<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator version="0.1">
	<model name="">
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.28"/>
		<metadata head="neck" headCap="-60,60" headCap1="-60,60" headAxis="2" headAxis2="0" headDir="-1"/>
		<phase name="quadWalk" leftHind="leg1" rightHind="leg2" leftFront="leg4" rightFront="leg3" duration="10"/>
		
		<customTex default="arcanine">
            <part name="Head" tex="arcaninehead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0,0.25"/>
            <animation part="Head" trigger="sleeping" diffs="0,0.50"/>
        </customTex>
		
		<phase type = "idle">
			<part name="tail">
                <component name="1" rotOffset="3,0,0" rotChange="-3,0,0" length="5" startKey="0"/>
                <component name="2" rotChange="3,0,0" length="5" startKey="5"/>
            </part>
		</phase>
		<phase type = "walking">
			<part name="tail">
                <component name="1" rotOffset="3,0,0" rotChange="-3,0,0" length="5" startKey="0"/>
                <component name="2" rotChange="3,0,0" length="5" startKey="5"/>
            </part>
			<part name="mouth">
                <component name="1" rotOffset="8,0,0" rotChange="4,0,0" length="5" startKey="0"/>
                <component name="2" rotChange="-4,0,0" length="5" startKey="5"/>
            </part>
		</phase>
		<merges merge="idle->walking" />
	</model>
</ModelAnimator>


