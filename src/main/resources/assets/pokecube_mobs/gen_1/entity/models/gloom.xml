<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator version="0.1">
    <model name="">
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.25"/>
        
        <phase name="biWalk" leftLeg="leg1" rightLeg="leg2" leftArm="" rightArm="" duration="6" legAngle="25"/>
        
		<customTex default="gloom">
            <part name="Head" tex="gloomhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0.45,0"/>
        </customTex>
		
        <phase type="idle">
            <part name="arm1">
               <component name="1" rotOffset="0,0,-40" rotChange="0,0,2" length="7" startKey="0"/>
               <component name="2" rotChange="0,0,-2" length="7" startKey="10"/>
            </part>
            <part name="arm2">
               <component name="1" rotOffset="0,0,40" rotChange="0,0,-2" length="7" startKey="0"/>
               <component name="2" rotChange="0,0,2" length="7" startKey="10"/>
            </part>
            
            <part name="leg1">
               <component name="1" rotOffset="0,14,0" length="10" startKey="0"/>
            </part>
            <part name="leg2">
               <component name="1" rotOffset="0,-14,0" length="10" startKey="0"/>
            </part>
            <part name="fl1">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
            <part name="fl2">
               <component name="1" rotChange="0,0,1" length="7" startKey="0"/>
               <component name="2" rotChange="0,0,-1" length="7" startKey="10"/>
            </part>
            <part name="fl3">
               <component name="1" rotChange="0,0,-1" length="7" startKey="0"/>
               <component name="2" rotChange="0,0,1" length="7" startKey="10"/>
            </part>
            <part name="fl4">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
            <part name="fl5">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
            <part name="fl6">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
        </phase>
        <phase type="walking">
            <part name="arm1">
               <component name="1" rotOffset="0,0,-40" rotChange="0,10,0" length="7" startKey="0"/>
               <component name="2" rotChange="0,-10,0" length="7" startKey="10"/>
            </part>
            <part name="arm2">
               <component name="1" rotOffset="0,0,40" rotChange="0,10,0" length="7" startKey="0"/>
               <component name="2" rotChange="0,-10,0" length="7" startKey="10"/>
            </part>
            
            <part name="fl1">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
            <part name="fl2">
               <component name="1" rotChange="0,0,1" length="7" startKey="0"/>
               <component name="2" rotChange="0,0,-1" length="7" startKey="10"/>
            </part>
            <part name="fl3">
               <component name="1" rotChange="0,0,-1" length="7" startKey="0"/>
               <component name="2" rotChange="0,0,1" length="7" startKey="10"/>
            </part>
            <part name="fl4">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
            <part name="fl5">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
            <part name="fl6">
               <component name="1" rotChange="1,0,0" length="7" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="7" startKey="10"/>
            </part>
        </phase>
    </model>
</ModelAnimator>
