<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator version="0.1">
    <model name="">
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.45"/>
        <phase name="biWalk" leftLeg="leg1" rightLeg="leg2" leftArm="" rightArm="" duration="10" legAngle="25"/>
		
		<customTex default="poliwhirl">
            <part name="Head" tex="poliwhirlhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0,0.32"/>
        </customTex>
		
		<phase type="idle">
			<part name="arm1">
                <component name="1" startKey="0" length="10" rotOffset="0,0,-55," rotChange="0,0,0.5,"/>
                <component name="2" startKey="10" length="10" rotChange="0,0,-0.5,"/>
            </part>
				<part name="hand1">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,-10,"/>
				</part>
			
			<part name="arm2">
                <component name="1" startKey="0" length="10" rotOffset="0,0,55," rotChange="0,0,-0.5,"/>
                <component name="2" startKey="10" length="10" rotChange="0,0,0.5,"/>
            </part>
				<part name="hand2">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,10,"/>
				</part>
			
			<part name="leg1">
				<component name="1" startKey="0" length="1" rotOffset="0,10,0,"/>
			</part>
			<part name="leg2">
				<component name="1" startKey="0" length="1" rotOffset="0,-10,0,"/>
			</part>			
        </phase>
		
		<phase type="sitting">
			
			<part name="body">
				<component name="1" startKey="0" length="1" posOffset="0,10,0,"/>
			</part>
			
			<part name="arm1">
                <component name="1" startKey="0" length="10" rotOffset="0,0,-55," rotChange="0,0,0.5,"/>
                <component name="2" startKey="10" length="10" rotChange="0,0,-0.5,"/>
            </part>
				<part name="hand1">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,15,"/>
				</part>
			
			<part name="arm2">
                <component name="1" startKey="0" length="10" rotOffset="0,0,55," rotChange="0,0,-0.5,"/>
                <component name="2" startKey="10" length="10" rotChange="0,0,0.5,"/>
            </part>
				<part name="hand2">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,-15,"/>
				</part>
			
			<part name="leg1">
				<component name="1" startKey="0" length="1" posOffset="0,0,-5," rotOffset="-90,10,0,"/>
			</part>
			<part name="leg2">
				<component name="1" startKey="0" length="1" posOffset="0,0,-5," rotOffset="-90,-10,0,"/>
			</part>			
        </phase>
		
		<phase type="walking">
			<part name="arm1">
                <component name="1" startKey="0" length="10" rotOffset="0,0,-55," rotChange="0,-40,0,"/>
                <component name="2" startKey="5" length="10" rotChange="0,80,0,"/>
				<component name="3" startKey="10" length="10" rotChange="0,-40,0,"/>
            </part>
				<part name="hand1">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,-10,"/>
				</part>
			
			<part name="arm2">
                <component name="1" startKey="0" length="10" rotOffset="0,0,55," rotChange="0,-40,0,"/>
                <component name="2" startKey="5" length="10" rotChange="0,80,0,"/>
				<component name="3" startKey="10" length="10" rotChange="0,-40,0,"/>
            </part>
				<part name="hand2">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,10,"/>
				</part>		
        </phase>
	</model>
</ModelAnimator>
