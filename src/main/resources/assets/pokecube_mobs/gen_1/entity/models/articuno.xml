        <phase name="snakeIdle" parts="tail:tail1:tail2:tail3:tail4:tail5:tail6:tail7:tail8" axis="0"/>
        
        <phase name="biWalk" leftLeg="leg1" rightLeg="leg2" leftArm="" rightArm="" duration="6" legAngle="25"/>
        
        <phase type="idle">            
            <part name="wing1">
               <component name="1" rotOffset="20,30,-80" length="10" startKey="0"/>
            </part>
                <part name="wing1_1">
                    <component name="1" rotOffset="0,0,-170" length="10" startKey="0"/>
                </part>
                    <part name="wing1_2">
                        <component name="1" rotOffset="0,0,170" length="10" startKey="0"/>
