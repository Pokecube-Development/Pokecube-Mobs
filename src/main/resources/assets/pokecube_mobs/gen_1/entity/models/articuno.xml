               <component name="2" rotChange="0,0,75" length="10" startKey="10"/>
			   <component name="3" rotChange="0,0,-35" length="10" startKey="20"/>
            </part>
                <part name="wing2_1">
                   <component name="1" rotChange="0,0,-20" length="10" startKey="0"/>
