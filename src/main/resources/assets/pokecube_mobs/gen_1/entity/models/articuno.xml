        </phase>
        <phase type="flying">
            <part name="body">
               <component name="1" rotOffset="8,0,0" length="1" startKey="0"/>
            </part>
            <part name="head_flame">
               <component name="1" rotOffset="8,0,0" rotChange="5,0,0" length="3" startKey="0"/>
               <component name="2" rotChange="-5,0,0" length="3" startKey="7"/>
            </part>
            
            <part name="wing1">
               <component name="1" rotOffset="10,0,0" rotChange="0,0,35" length="10" startKey="0"/>
