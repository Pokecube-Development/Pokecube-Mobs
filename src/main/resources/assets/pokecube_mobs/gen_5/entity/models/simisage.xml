<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.13"/>
        <metadata head="head" headCap="-80,80" headCap1="-50,50" headAxis="2" headAxis2="0" headDir="-1"/>
        <phase name="biWalk" leftArm="" rightArm="" leftLeg="leg1" rightLeg="leg2" duration="10" legAngle="25" armAngle="20"/>
		<phase name="snakeWalk" parts="tail:tail1:tail2:tail3:tail4" angle="15" axis="0" length="20"/>
		
		<customTex default="simisage">
            <part name="Head" tex="simisagehead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0,0.5"/>
        </customTex>
		
		<phase type="idle">
				<part name="orl1">
					<component name="1" startKey="0" length="10" rotOffset="0,0,0," rotChange="0,0,-3,"/>
					<component name="2" startKey="10" length="10" rotChange="0,0,3,"/>
				</part>
				<part name="orl2">
					<component name="1" startKey="0" length="10" rotOffset="0,0,0," rotChange="0,0,3,"/>
					<component name="2" startKey="10" length="10" rotChange="0,0,-3,"/>
				</part>
				
				<part name="arm1">
					<component name="1" startKey="0" length="10" rotOffset="0,0,-45," rotChange="0,0,-3,"/>
					<component name="2" startKey="10" length="10" rotChange="0,0,3,"/>
				</part>
					<part name="arm1_1">
						<component name="1" startKey="0" length="1" rotOffset="10,-30,0,"/>
					</part>
				<part name="arm2">
					<component name="1" startKey="0" length="10" rotOffset="0,0,45," rotChange="0,0,3,"/>
					<component name="2" startKey="10" length="10" rotChange="0,0,-3,"/>
				</part>
					<part name="arm2_1">
						<component name="1" startKey="0" length="1" rotOffset="10,30,0,"/>
					</part>
				
			<part name="tail">
				<component name="1" rotOffset="-20,0,0" rotChange="-3,0,0" length="10" startKey="0"/>
				<component name="2" rotChange="3,0,0" length="10" startKey="10"/>
            </part>
				<part name="tail1">
					<component name="1" rotOffset="15,0,0" rotChange="-3,0,0" length="10" startKey="0"/>
					<component name="2" rotChange="3,0,0" length="10" startKey="10"/>
				</part>
					<part name="tail2">
						<component name="1" rotOffset="15,0,0" rotChange="-3,0,0" length="10" startKey="0"/>
						<component name="2" rotChange="3,0,0" length="10" startKey="10"/>
					</part>
						<part name="tail3">
							<component name="1" rotOffset="15,0,0" rotChange="-3,0,0" length="10" startKey="0"/>
							<component name="2" rotChange="3,0,0" length="10" startKey="10"/>
						</part>
							<part name="tail4">
								<component name="1" rotOffset="15,0,0" rotChange="-3,0,0" length="10" startKey="0"/>
								<component name="2" rotChange="3,0,0" length="10" startKey="10"/>
							</part>
		</phase>
		<phase type="walking">
			<part name="orl1">
					<component name="1" startKey="0" length="10" rotOffset="0,0,0," rotChange="0,0,-3,"/>
					<component name="2" startKey="10" length="10" rotChange="0,0,3,"/>
			</part>
			<part name="orl2">
				<component name="1" startKey="0" length="10" rotOffset="0,0,0," rotChange="0,0,3,"/>
				<component name="2" startKey="10" length="10" rotChange="0,0,-3,"/>
			</part>
			
			<part name="arm1">
				<component name="1" startKey="0" length="10" rotOffset="0,0,-45," rotChange="0,-7,0,"/>
				<component name="2" startKey="10" length="10" rotChange="0,7,0,"/>
			</part>
				<part name="arm1_1">
					<component name="1" startKey="0" length="1" rotOffset="10,-30,0,"/>
				</part>
			<part name="arm2">
				<component name="1" startKey="0" length="10" rotOffset="0,0,45," rotChange="0,-7,0,"/>
				<component name="2" startKey="10" length="10" rotChange="0,7,0,"/>
			</part>
				<part name="arm2_1">
					<component name="1" startKey="0" length="1" rotOffset="10,30,0,"/>
				</part>
		</phase>
	</model>
</ModelAnimator>
