<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator version="0.1">
   <model>
		<phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.39"/>
	
		<phase name="biWalk" leftLeg="foot1" rightLeg="foot2" leftArm="" rightArm="" duration="6" legAngle="25"/>
		
		<customTex default="trubbish">
            <part name="Head" tex="trubbishhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0.3,0"/>
        </customTex>
		
		<phase type="idle">
			<part name="body">
               <component name="1" rotChange="1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="10" startKey="15"/>
            </part>
			
			<part name="arm1">
               <component name="1" rotOffset="60,40,-70" rotChange="2,0,0" length="5" startKey="0"/>
               <component name="2" rotChange="-2,0,0" length="5" startKey="10"/>
            </part>
			<part name="arm2">
               <component name="1" rotOffset="60,-40,70" rotChange="2,0,0" length="5" startKey="0"/>
               <component name="2" rotChange="-2,0,0" length="5" startKey="10"/>
            </part>
		</phase>
		<phase type="walking">
			<part name="arm1">
               <component name="1" rotOffset="60,40,-70" rotChange="2,0,0" length="5" startKey="0"/>
               <component name="2" rotChange="-2,0,0" length="5" startKey="10"/>
            </part>
			<part name="arm2">
               <component name="1" rotOffset="60,-40,70" rotChange="2,0,0" length="5" startKey="0"/>
               <component name="2" rotChange="-2,0,0" length="5" startKey="10"/>
            </part>
		</phase>
   </model>
</ModelAnimator>
