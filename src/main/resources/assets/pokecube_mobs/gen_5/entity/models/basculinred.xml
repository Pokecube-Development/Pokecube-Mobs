<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0.5" scale="0.60"/>
		
		<customTex default="basculinred">
            <part name="Head" tex="basculinrhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0.4,0"/>
        </customTex>
		
		<phase type="idle">
			<part name="mouth">
               <component name="1" rotChange="5,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="-5,0,0" length="10" startKey="10"/>
            </part>
			
			<part name="wing1">
               <component name="1" rotChange="0,12,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,-17,0" length="10" startKey="10"/>
            </part>
			<part name="wing2">
               <component name="1" rotChange="0,-17,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,12,0" length="10" startKey="10"/>
            </part>
			<part name="mouth">
               <component name="1" rotChange="7,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="-7,0,0" length="10" startKey="10"/>
            </part>
			
            <part name="tail">
               <component name="1" rotChange="0,25,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,-25,0" length="10" startKey="10"/>
            </part>
        </phase>
		<phase type="swimming">
			<part name="mouth">
               <component name="1" rotChange="5,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="-5,0,0" length="10" startKey="10"/>
            </part>
			
			<part name="wing1">
               <component name="1" rotChange="0,12,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,-17,0" length="10" startKey="10"/>
            </part>
			<part name="wing2">
               <component name="1" rotChange="0,-17,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,12,0" length="10" startKey="10"/>
            </part>
			<part name="mouth">
               <component name="1" rotChange="7,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="-7,0,0" length="10" startKey="10"/>
            </part>
			
            <part name="tail">
               <component name="1" rotChange="0,25,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,-25,0" length="10" startKey="10"/>
            </part>
        </phase>
    </model>
</ModelAnimator>
