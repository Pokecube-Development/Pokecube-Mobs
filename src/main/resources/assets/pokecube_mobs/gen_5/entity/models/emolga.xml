<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.3"/>
        <metadata head="head" headCap="-80,80" headCap1="-50,50" headAxis="2" headAxis2="0" headDir="-1"/>
		
		<customTex default="emolga">
            <part name="Head" tex="emolgahead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0.49,0"/>
        </customTex>
		
		<phase name="biWalk" leftLeg="foot2" rightLeg="foot1" leftArm="" rightArm="" duration="10" legAngle="15"/>
		<phase type="idle">
				<part name="arm1">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,-60,"/>
				</part>
				<part name="arm2">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,60,"/>
				</part>
			<part name="tail">
                <component name="1" startKey="0" length="10" rotChange="3,0,0,"/>
                <component name="2" startKey="10" length="10" rotChange="-3,0,0,"/>
            </part>
        </phase>
		<phase type="walking">
				<part name="arm1">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,-60,"/>
				</part>
				<part name="arm2">
				   <component name="1" startKey="0" length="1" rotOffset="0,0,60,"/>
				</part>
			<part name="tail">
                <component name="1" startKey="0" length="10" rotChange="0,3,0,"/>
                <component name="2" startKey="10" length="10" rotChange="0,-3,0,"/>
            </part>
        </phase>
		<phase type="flying">
            <part name="body">
                <component name="1" startKey="0" length="10" rotOffset="50,0,0,"/>
            </part>
				<part name="tail">
					<component name="1" startKey="0" length="10" rotOffset="-35,0,0," rotChange="4,0,0,"/>
					<component name="2" startKey="10" length="10" rotChange="-4,0,0,"/>
				</part>
				
				<part name="foot1">
				   <component name="1" startKey="0" length="1" rotOffset="40,0,5,"/>
				</part>

				<part name="foot2">
				   <component name="1" startKey="0" length="1" rotOffset="40,0,-5,"/>
				</part>

        </phase>
    </model>
</ModelAnimator>
