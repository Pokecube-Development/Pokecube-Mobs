<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="1.2"/>
        <metadata head="head" headCap="-90,90" headCap1="-50,50" headAxis="2" headAxis2="-1" headDir="-1"/>
		
		<phase name="snakeWalk" parts="tail:tail1:tail2" axis="2"/>
		<phase name="snakeFly" parts="tail:tail1:tail2" axis="0"/>
		
		<customTex default="yveltal">
            <part name="Head" tex="yveltalhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0,0.5"/>
        </customTex>
		
		<phase name="biWalk" leftLeg="leg1:leg1_1" rightLeg="leg2:leg2_1" leftArm="" rightArm="" duration="6" legAngle="25"/>
		
		<phase type="sitting">
			<part name="body">
               <component name="1" startKey="0" length="1" posOffset="0,-4,-6,"/>
            </part>
			<part name="neck">
			   <component name="1" startKey="0" length="1" rotOffset="50,0,0,"/>
			</part>
			
			<part name="tail">
               <component name="1" startKey="0" length="1" rotOffset="-40,0,0,"/>
            </part>
				<part name="tail1">
				   <component name="1" startKey="0" length="1" rotOffset="-60,0,0,"/>
				</part>
				<part name="tail2">
				   <component name="1" startKey="0" length="1" rotOffset="-75,0,0,"/>
				</part>
			
			<part name="leg1">
               <component name="1" startKey="0" length="1" rotOffset="15,0,0,"/>
            </part>
				<part name="leg1_1">
				   <component name="1" startKey="0" length="1" rotOffset="-45,0,0,"/>
				</part>
			<part name="leg2">
               <component name="1" startKey="0" length="1" rotOffset="15,0,0,"/>
            </part>
				<part name="leg2_1">
				   <component name="1" startKey="0" length="1" rotOffset="-45,0,0,"/>
				</part>
			
			<part name="wing1">
               <component name="1" rotOffset="40,-80,0" length="10" startKey="0"/>
            </part>
                <part name="wing1_1">
                    <component name="1" rotOffset="0,0,130" length="10" startKey="0"/>
                </part>
			<part name="wing2">
               <component name="1" rotOffset="40,80,0" length="10" startKey="0"/>
            </part>
                <part name="wing2_1">
                    <component name="1" rotOffset="0,0,-130" length="10" startKey="0"/>
                </part>		
        </phase>
		
		<phase type="idle">
			<part name="body">
               <component name="1" startKey="0" length="1" posOffset="0,-10,0," rotOffset="70,0,0,"/>
            </part>
				<part name="p">
				   <component name="1" rotOffset="-20,0,0," rotChange="30,0,0" length="10" startKey="0"/>
				   <component name="2" rotChange="-60,0,0" length="10" startKey="5"/>
				   <component name="3" rotChange="30,0,0" length="10" startKey="10"/>
				</part>
				<part name="neck">
				   <component name="1" startKey="0" length="1" rotOffset="-50,0,0,"/>
				</part>
			
			<part name="tail">
               <component name="1" startKey="0" length="1" rotOffset="-10,0,0,"/>
            </part>
				<part name="tail1">
				   <component name="1" startKey="0" length="1" rotOffset="15,0,0,"/>
				</part>
				<part name="tail2">
				   <component name="1" startKey="0" length="1" rotOffset="10,0,0,"/>
				</part>
			
			<part name="leg1">
               <component name="1" startKey="0" length="1" rotOffset="15,0,0,"/>
            </part>
				<part name="leg1_1">
				   <component name="1" startKey="0" length="1" rotOffset="-25,0,0,"/>
				</part>
			<part name="leg2">
               <component name="1" startKey="0" length="1" rotOffset="15,0,0,"/>
            </part>
				<part name="leg2_1">
				   <component name="1" startKey="0" length="1" rotOffset="-25,0,0,"/>
				</part>
			
			<part name="wing1">
               <component name="1" rotOffset="10,40,-80" length="10" startKey="0"/>
            </part>
                <part name="wing1_1">
                    <component name="1" rotOffset="0,-15,150" length="10" startKey="0"/>
                </part>
			<part name="wing2">
               <component name="1" rotOffset="10,-40,80" length="10" startKey="0"/>
            </part>
                <part name="wing2_1">
                    <component name="1" rotOffset="0,15,-150" length="10" startKey="0"/>
                </part>		
        </phase>
		
		<phase type="walking">
			<part name="body">
               <component name="1" startKey="0" length="1" posOffset="0,-10,0," rotOffset="70,0,0,"/>
            </part>
				<part name="p">
				   <component name="1" rotOffset="-20,0,0," rotChange="30,0,0" length="10" startKey="0"/>
				   <component name="2" rotChange="-60,0,0" length="10" startKey="5"/>
				   <component name="3" rotChange="30,0,0" length="10" startKey="10"/>
				</part>
				<part name="neck">
				   <component name="1" startKey="0" length="1" rotOffset="-50,0,0,"/>
				</part>
				
			<part name="wing1">
               <component name="1" rotOffset="10,40,-80" length="10" startKey="0"/>
			   <component name="1" rotChange="0,-10,0" length="10" startKey="0"/>
			   <component name="2" rotChange="0,10,0" length="10" startKey="10"/>
            </part>
                <part name="wing1_1">
                    <component name="1" rotOffset="0,-15,150" length="10" startKey="0"/>
                </part>
			<part name="wing2">
               <component name="1" rotOffset="10,-40,80" length="10" startKey="0"/>
			   <component name="1" rotChange="0,-20,0" length="10" startKey="0"/>
			   <component name="2" rotChange="0,20,0" length="10" startKey="10"/>
            </part>
                <part name="wing2_1">
                    <component name="1" rotOffset="0,15,-150" length="10" startKey="0"/>
                </part>	
        </phase>
		
		<phase type="flying">
			<part name="body">
               <component name="1" startKey="0" length="1" posOffset="0,-10,0," rotOffset="70,0,0,"/>
            </part>
				<part name="p">
				   <component name="1" rotOffset="-20,0,0," rotChange="30,0,0" length="10" startKey="0"/>
				   <component name="2" rotChange="-60,0,0" length="10" startKey="5"/>
				   <component name="3" rotChange="30,0,0" length="10" startKey="10"/>
				</part>
				<part name="neck">
				   <component name="1" startKey="0" length="1" rotOffset="-50,0,0,"/>
				</part>
			
			<part name="leg1">
			   <component name="1" rotOffset="30,0,0" rotChange="10,0,0" length="10" startKey="0"/>
			   <component name="2" rotChange="-20,0,0" length="10" startKey="5"/>
			   <component name="3" rotChange="10,0,0" length="10" startKey="10"/>
            </part>
				<part name="leg1_1">
				   <component name="1" rotOffset="-80,0,0" length="10" startKey="0"/>
				</part>
					<part name="foot1">
					   <component name="1" rotOffset="15,0,0" length="10" startKey="0"/>
					</part>
			<part name="leg2">
               <component name="1" rotOffset="30,0,0" rotChange="10,0,0" length="10" startKey="0"/>
			   <component name="2" rotChange="-20,0,0" length="10" startKey="5"/>
			   <component name="3" rotChange="10,0,0" length="10" startKey="10"/>
            </part>
				<part name="leg2_1">
				   <component name="1" rotOffset="-80,0,0" length="10" startKey="0"/>
				</part>
					<part name="foot2">
					   <component name="1" rotOffset="15,0,0" length="10" startKey="0"/>
					</part>
			
			<part name="wing1">
               <component name="1" rotOffset="25,0,0" rotChange="0,-50,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,100,0" length="10" startKey="5"/>
			   <component name="3" rotChange="0,-50,0" length="10" startKey="10"/>
            </part>
                <part name="wing1_1">
                    <component name="1" rotOffset="0,8,0" rotChange="0,-15,0" length="10" startKey="0"/>
                    <component name="2" rotChange="0,15,0" length="10" startKey="10"/>
                </part>
            
            <part name="wing2">
               <component name="1" rotOffset="25,0,0" rotChange="0,50,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,-100,0" length="10" startKey="5"/>
			   <component name="3" rotChange="0,50,0" length="10" startKey="10"/>
            </part>
                <part name="wing2_1">
                    <component name="1" rotOffset="0,-8,0" rotChange="0,15,0" length="10" startKey="0"/>
                    <component name="2" rotChange="0,-15,0" length="10" startKey="10"/>
                </part>
        </phase>
    </model>
</ModelAnimator>