<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.9"/>
        <metadata headCap="-60,60" headAxis="2" headAxis2="0" headDir="-1" head="head"/>
        <phase name="biWalk" leftLeg="leg1" rightLeg="leg2" leftArm="" rightArm="" duration="6" legAngle="25"/>
		
		<customTex default="tyrantrum">
            <part name="Head" tex="tyrantrumhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0,0.45"/>
        </customTex>
		
		<phase name="snakeIdle" parts="tail:tail1:tail2:tail3" axis="0"/>
		<phase name="snakeWalk" parts="tail:tail1:tail2:tail3" axis="1"/>
		
		<phase type="idle">
			<part name="mouth">
                <component name="1" rotChange="5,0,0" length="10" startKey="0"/>
                <component name="2" rotChange="-5,0,0" length="10" startKey="10"/>
            </part>
				<part name="arm2">
					<component name="1" rotChange="0,0,-5" length="10" startKey="0"/>
					<component name="2" rotChange="0,0,5" length="10" startKey="10"/>
				</part>
				<part name="arm1">
					<component name="1" rotChange="0,0,5" length="10" startKey="0"/>
					<component name="2" rotChange="0,0,-5" length="10" startKey="10"/>
				</part>
        </phase>
		
		<phase type="walking">
            <part name="mouth">
                <component name="1" rotChange="5,0,0" length="10" startKey="0"/>
                <component name="2" rotChange="-5,0,0" length="10" startKey="10"/>
            </part>
				<part name="arm2">
					<component name="1" rotChange="0,0,-5" length="10" startKey="0"/>
					<component name="2" rotChange="0,0,5" length="10" startKey="10"/>
				</part>
				<part name="arm1">
					<component name="1" rotChange="0,0,5" length="10" startKey="0"/>
					<component name="2" rotChange="0,0,-5" length="10" startKey="10"/>
				</part>
        </phase>
    </model>
</ModelAnimator>
