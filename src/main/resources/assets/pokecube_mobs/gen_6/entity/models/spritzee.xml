<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator version="0.1">
   <model>
		<phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.22"/>
	
		<phase name="biWalk" leftLeg="foot1" rightLeg="foot2" leftArm="" rightArm="" duration="6" legAngle="25"/>
		
		<customTex default="spritzee">
            <part name="Head" tex="spritzeehead"/>
            <animation part="Head" trigger="random:0.005:20" diffs="0.5,0"/>
        </customTex>
		
		<phase type="idle">		
			<part name="wing1">
               <component name="1" rotOffset="20,80,0" length="10" startKey="0"/>
            </part>
			<part name="wing2">
               <component name="1" rotOffset="20,-80,0" length="10" startKey="0"/>
            </part>		
			<part name="top">
               <component name="1" rotOffset="2,0,0" rotChange="-8,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="8,0,0" length="10" startKey="10"/>
            </part>
		</phase>
		<phase type="walking">
			<part name="wing1">
               <component name="1" rotOffset="20,80,0" length="10" startKey="0"/>
            </part>
			<part name="wing2">
               <component name="1" rotOffset="20,-80,0" length="10" startKey="0"/>
            </part>		
			<part name="top">
               <component name="1" rotOffset="2,0,0" rotChange="-8,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="8,0,0" length="10" startKey="10"/>
            </part>
		</phase>
		<phase type="flying">
			<part name="wing1">
               <component name="1" rotOffset="20,0,5" rotChange="0,0,10" length="5" startKey="0"/>
               <component name="2" rotChange="0,0,-10" length="5" startKey="10"/>
            </part>
			<part name="wing2">
               <component name="1" rotOffset="20,0,-5" rotChange="0,0,-10" length="5" startKey="0"/>
               <component name="2" rotChange="0,0,10" length="5" startKey="10"/>
            </part>
			<part name="top">
               <component name="1" rotOffset="2,0,0" rotChange="-8,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="8,0,0" length="10" startKey="10"/>
            </part>
		</phase>
   </model>
</ModelAnimator>
