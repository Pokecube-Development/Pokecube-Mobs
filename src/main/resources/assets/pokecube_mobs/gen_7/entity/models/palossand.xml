<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.55"/>
        <metadata head="head" headCap="-180,180" headCap1="-5,5" headAxis="2" headAxis2="0" headDir="1"/>
        <phase name="biWalk" leftLeg="t1" rightLeg="t2" leftArm="" rightArm="" duration="20" legAngle="5"/>
		
		<customTex default="palossand">
            <part name="Head" tex="palossandhead"/>
            <animation part="Head" trigger="random:0.005:25" diffs="0,0.29"/>
        </customTex>
		
		<phase type="idle">
			<part name="pa">
                <component name="1" rotChange="0,-360,0" length="120" startKey="0"/>
            </part>
			
			<part name="t1">
               <component name="1" rotChange="0,0,-5" length="10" startKey="0"/>
               <component name="2" rotChange="0,0,5" length="10" startKey="10"/>
            </part>
			<part name="t2">
               <component name="1" rotChange="0,0,5" length="10" startKey="0"/>
               <component name="2" rotChange="0,0,-5" length="10" startKey="10"/>
            </part>
		</phase>
		
		<phase type="walking">
			<part name="pa">
                <component name="1" rotChange="0,-360,0" length="120" startKey="0"/>
            </part>
		</phase>	
    </model>
</ModelAnimator>
