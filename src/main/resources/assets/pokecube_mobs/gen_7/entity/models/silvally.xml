<?xml version="1.0" encoding="UTF-8"?>
<ModelAnimator>
    <model>
        <phase name="global" rotation="1,0,0,0" offset="0,0,0" scale="0.28"/>
		
		<customTex default="silvally">
			<part name="Head" tex="silvallyh"/>
			<animation part="Head" trigger="random:0.005:20" diffs="0,0.35"/>
			
            <forme name="Silvally" tex="silvally"/>			
            <forme name="Silvally Bug" tex="silvallybug"/>
            <forme name="Silvally Dark" tex="silvallydark"/>
            <forme name="Silvally Dragon" tex="silvallydragon"/>
            <forme name="Silvally Electric" tex="silvallyelectric"/>
            <forme name="Silvally Fairy" tex="silvallyfairy"/>
            <forme name="Silvally Fighting" tex="silvallyfighting"/>
            <forme name="Silvally Fire" tex="silvallyfire"/>
            <forme name="Silvally Flying" tex="silvallyflying"/>
            <forme name="Silvally Ghost" tex="silvallyghost"/>
            <forme name="Silvally Grass" tex="silvallygrass"/>
            <forme name="Silvally Ground" tex="silvallyground"/>
            <forme name="Silvally Ice" tex="silvallyice"/>
            <forme name="Silvally Poison" tex="silvallypoison"/>
            <forme name="Silvally Psychic" tex="silvallypsychic"/>
            <forme name="Silvally Rock" tex="silvallyrock"/>
            <forme name="Silvally Steel" tex="silvallysteel"/>
            <forme name="Silvally Water" tex="silvallywater"/>			
        </customTex>
		
		<metadata head="head" headCap="-80,80" headCap1="-50,50" headAxis="2" headAxis2="0" headDir="-1"/>
        <metadata head="neck" headCap="-30,30" headCap1="-10,10" headAxis="2" headAxis2="0" headDir="-1"/>
		<metadata head="neck1" headCap="-20,20" headCap1="-10,10" headAxis="2" headAxis2="0" headDir="-1"/>
		<metadata head="neck2" headCap="-10,10" headCap1="-5,5" headAxis="2" headAxis2="0" headDir="-1"/>
		
		<phase name="quadWalk" leftHind="leg1" rightHind="leg2" leftFront="leg4" rightFront="leg3" duration="10"/>
		
		<phase type="idle">
			<part name="tail">
               <component name="1" rotOffset="6,0,0" rotChange="-8,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="8,0,0" length="10" startKey="10"/>
            </part>
			<part name="orl1">
               <component name="1" rotOffset="0,0,0" rotChange="-1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="1,0,0" length="10" startKey="10"/>
            </part>
			<part name="orl2">
               <component name="1" rotOffset="0,0,0" rotChange="-1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="1,0,0" length="10" startKey="10"/>
            </part>
			
			<part name="top">
               <component name="1" rotOffset="0,0,0" rotChange="-1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="1,0,0" length="10" startKey="10"/>
            </part>
				<part name="mouth">
				   <component name="1" rotOffset="2,0,0" rotChange="-8,0,0" length="10" startKey="0"/>
				   <component name="2" rotChange="8,0,0" length="10" startKey="10"/>
				</part>
		</phase>
		<phase type="walking">
			<part name="tail">
               <component name="1" rotOffset="0,-5,0" rotChange="0,-5,0" length="10" startKey="0"/>
               <component name="2" rotChange="0,5,0" length="10" startKey="10"/>
            </part>
			<part name="orl1">
               <component name="1" rotOffset="0,0,0" rotChange="-1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="1,0,0" length="10" startKey="10"/>
            </part>
			<part name="orl2">
               <component name="1" rotOffset="0,0,0" rotChange="-1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="1,0,0" length="10" startKey="10"/>
            </part>
			
			<part name="top">
               <component name="1" rotOffset="8,0,0" rotChange="1,0,0" length="10" startKey="0"/>
               <component name="2" rotChange="-1,0,0" length="10" startKey="10"/>
            </part>
				<part name="mouth">
				   <component name="1" rotOffset="2,0,0" rotChange="-8,0,0" length="10" startKey="0"/>
				   <component name="2" rotChange="8,0,0" length="10" startKey="10"/>
				</part>
		</phase>
    </model>
</ModelAnimator>
